{
  "name": "fast-company-server",
  "version": "1.0.0",
  "description": "Server for fast-company application",
  "main": "index.js",
  "scripts": {
    "server": "nodemon app.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "start": "node server.js",
    "install-client": "cd client && npm install",
    "build": "cd client && npm run build",
    "postbuild": "npm install && npm run install-client && npm run build"
  },
  "keywords": [
    "nodeJS"
  ],
  "author": "Roman Burkeev",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "chalk": "^4.1.2",
    "config": "^3.3.8",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.7.3"
  },
  "devDependencies": {
    "concurrently": "^7.6.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20"
  }
}
